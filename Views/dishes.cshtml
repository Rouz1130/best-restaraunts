<h1>All dishes</h1>
@if (@Model.Count == 0)
{
  <p>There are no dishes.</p>
}
<ul>
  @foreach (var dish in Model)
  {
  <li><a href="/stores/@dish.GetId()">@dish.GetId()</a></li>
    <li>@dish.GetName()</li>
    <li>@dish.GetPrice()</li>
    <li>@dish.GetStoreId()</li>
    <li><a href="/dishes/edit/@dish.GetId()">Edit</a></li>
    <li><a href="/dish/delete/@dish.GetId()">Delete</a></li>

  }
</ul>
<a href="/dishes/new">Add new dish</a>
