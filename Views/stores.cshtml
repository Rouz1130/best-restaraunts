<h1>All Stores</h1>
@if (@Model.Count == 0)
{
  <p>There are no stores.</p>
}
<ul>
  @foreach (var store in Model)
  {
    <li><a href="/stores/@store.GetId()">@store.GetName()</a></li>
    <li><a href="/store/edit/@store.GetId()">Edit</a></li>
    <li><a href="/store/delete/@store.GetId()">Delete</a></li>

  }
</ul>
<a href="/stores/new">Add new store</a>
